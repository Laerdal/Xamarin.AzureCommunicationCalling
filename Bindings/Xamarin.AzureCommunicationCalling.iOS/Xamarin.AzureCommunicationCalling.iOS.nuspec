    <license type="expression">MIT</license>
    <tags>Xamarin Azure Communication Services iOS</tags>
    <dependencies>
        <group targetFramework="Xamarin.iOS0.0">
        </group>
    </dependencies>
  </metadata>
  <files>
      <file src="bin/Release/Xamarin.AzureCommunicationCalling.iOS.dll" target="lib\Xamarin.iOS" />
      <file src="bin/Release/Xamarin.AzureCommunicationCalling.iOS.pdb" target="lib\Xamarin.iOS" />
  </files>
</package>
